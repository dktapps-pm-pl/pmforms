{
	"name": "dktapps/pmforms",
	"type": "library",
	"description": "Form API library for PocketMine-MP plugins",
	"license": "LGPL-3.0",
	"minimum-stability": "dev",
	"require": {
		"pocketmine/pocketmine-mp": "^4.0.0 || ^5.0.0"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.2.0",
		"phpstan/phpstan-strict-rules": "^1.0",
		"phpstan/extension-installer": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"dktapps\\pmforms\\": "src/"
		}
	},
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	},
	"extra":{
		"virion": {
			"spec": "3.0",
			"namespace-root": "dktapps\\pmforms"
		}
	}
}
